{
  "name": "desktop-reactjs-boilerplate",
  "version": "2.0.0",
  "private": true,
  "description": ":electron: (2022) Reactjs 18 with Redux.",
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "<com.desktop-reactjs-boilerplate>"
  },
  "bugs": {
    "url": "https://github.com/kkamara/desktop-reactjs-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/kkamara/desktop-reactjs-boilerplate"
  },
  "license": "MIT",
  "author": "https://www.github.com/kkamara/desktop-reactjs-boilerplate",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run build && electron-builder --linux --win --mac --x64 --ia32 --publish never",
    "builder:build": "electron-builder --win --x64 --publish never",
    "builder:build:windows": "electron-builder --win --x64 --publish never",
    "build": "npm run react-build && npm run builder:build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.2.2",
    "bootstrap": "^5.3",
    "jquery": "^3.6.3",
    "moment": "^2.29.2",
    "react": "^18.0.0",
    "react-bootstrap": "^2.7",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-paginate": "^8.1.3",
    "react-redux": "^7.2.8",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "wait-on": "^8.0.2"
  }
}
